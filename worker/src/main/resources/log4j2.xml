<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">
  <appenders>
    <ComAppender name="ComAppender">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </ComAppender>
  </appenders>
  <loggers>
    <root level="debug">
      <appender-ref ref="ComAppender"/>
    </root>
  </loggers>
</configuration>
